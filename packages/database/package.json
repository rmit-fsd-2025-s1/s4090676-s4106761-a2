{
  "name": "@repo/database",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "lint": "eslint",
    "lint-staged": "pretty-quick --staged",
    "prepare": "npm run build",
    "dev": "tsc -p tsconfig.build.json --watch"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./testEntity": {
      "import": "./dist/testEntity.js",
      "types": "./dist/testEntity.d.ts"
    }
  },
  "dependencies": {
    "@repo/types": "0.0.1",
    "mysql2": "^3.14.1",
    "typeorm": "^0.3.24"
  }
}
