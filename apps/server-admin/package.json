{
  "name": "server-admin",
  "version": "0.0.1",
  "private": "true",
  "type": "module",
  "scripts": {
    "watch-ts": "tsx watch --env-file=../../.env ./src/index.ts",
    "type-check": "tsc --noEmit",
    "lint": "eslint --fix",
    "lint-staged": "pretty-quick --staged",
    "watch-codegen": "graphql-codegen --config ./codegen.ts --watch",
    "dev": "concurrently \"npm run watch-ts\" \"npm run watch-codegen\"",
    "prepare": "graphql-codegen --config ./codegen.ts"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.7",
    "@graphql-codegen/typescript": "^4.1.6",
    "@graphql-codegen/typescript-resolvers": "^4.5.1",
    "@parcel/watcher": "^2.5.1",
    "@repo/lint-config": "*",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/graphql": "^14.2.3",
    "@types/node": "^22.15.17"
  },
  "dependencies": {
    "@apollo/server": "^4.12.2",
    "@as-integrations/express5": "^1.1.0",
    "@graphql-tools/schema": "^9.0.19",
    "@repo/database": "0.0.1",
    "@types/cookie": "^0.6.0",
    "cookie": "^0.7.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "graphql": "^16.11.0",
    "graphql-subscriptions": "^3.0.0",
    "graphql-ws": "^6.0.5",
    "ws": "^8.18.2"
  }
}
